# Staging Environment Configuration
# PATCH 159.0 - Environment Separation
# 
# This file is for STAGING/QA environment
# Set these variables in your Vercel project settings for staging branch

# ===============================================
# ENVIRONMENT IDENTIFIER
# ===============================================
VITE_NODE_ENV=production
VITE_APP_ENV=staging

# ===============================================
# SUPABASE CONFIGURATION - STAGING
# ===============================================
# Staging project: nautilus-staging
VITE_SUPABASE_URL=https://your-staging-project.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=your-staging-anon-key
VITE_SUPABASE_KEY=your-staging-anon-key
VITE_SUPABASE_PROJECT_ID=your-staging-project-id

# Backend (for scripts)
SUPABASE_URL=https://your-staging-project.supabase.co
SUPABASE_KEY=your-staging-service-role-key

# ===============================================
# APPLICATION URLS
# ===============================================
VITE_APP_URL=https://nautilus-staging.vercel.app
VITE_API_URL=https://nautilus-staging.vercel.app/api

# ===============================================
# OPENAI CONFIGURATION - STAGING
# ===============================================
VITE_OPENAI_API_KEY=sk-proj-staging-...

# ===============================================
# EXTERNAL SERVICES - STAGING
# ===============================================
# Mapbox - Staging token
VITE_MAPBOX_ACCESS_TOKEN=pk.eyJ1...staging
VITE_MAPBOX_TOKEN=pk.eyJ1...staging
MAPBOX_PUBLIC_TOKEN=pk.eyJ1...staging

# OpenWeather - Staging key
VITE_OPENWEATHER_API_KEY=your-staging-key
OPENWEATHER_API_KEY=your-staging-key

# ===============================================
# MONITORING & LOGGING - STAGING
# ===============================================
# Sentry - Staging environment
VITE_SENTRY_DSN=https://your-sentry-dsn@o0.ingest.sentry.io/staging
SENTRY_ORG=your-org
SENTRY_PROJECT=nautilus-staging
SENTRY_AUTH_TOKEN=your-auth-token
SENTRY_ENVIRONMENT=staging

# ===============================================
# EMAIL SERVICES - STAGING
# ===============================================
# Use test recipients in staging
EMAIL_HOST=smtp.yourdomain.com
EMAIL_PORT=587
EMAIL_USER=staging@nautilus.ai
EMAIL_PASS=your-staging-pass
EMAIL_FROM=staging@nautilus.ai
EMAIL_TO=qa-team@nautilus.ai

# Resend - Staging mode
RESEND_API_KEY=re_staging_...

# Admin emails for staging
ADMIN_EMAIL=admin-staging@nautilus.ai
SGSO_REPORT_EMAILS=qa-sgso@nautilus.ai

# ===============================================
# MQTT BROKER - STAGING
# ===============================================
# Use secure broker for staging
VITE_MQTT_URL=wss://mqtt-staging.nautilus.ai:8883/mqtt
VITE_MQTT_USER=nautilus-staging
VITE_MQTT_PASS=your-staging-mqtt-password

# ===============================================
# AUTHENTICATION
# ===============================================
JWT_SECRET=staging-secret-key-min-32-chars-must-be-secure-and-unique

# Embed token for protected routes
VITE_EMBED_ACCESS_TOKEN=staging-embed-token-secure

# ===============================================
# FEATURE FLAGS - STAGING
# ===============================================
# Test all features in staging
VITE_ENABLE_VOICE=true
VITE_ENABLE_AI_CHAT=true
VITE_ENABLE_TRAVEL_API=true
VITE_ENABLE_DEBUG=false
VITE_ENABLE_MOCK_DATA=false

# ===============================================
# FIREBASE - STAGING
# ===============================================
VITE_FIREBASE_API_KEY=your-staging-firebase-key
VITE_FIREBASE_AUTH_DOMAIN=nautilus-staging.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=nautilus-staging
VITE_FIREBASE_STORAGE_BUCKET=nautilus-staging.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id
VITE_FIREBASE_MEASUREMENT_ID=G-STAGING
VITE_FIREBASE_VAPID_KEY=your-vapid-key

# ===============================================
# STAGING SPECIFIC
# ===============================================
# Enable analytics in staging
VITE_ENABLE_ANALYTICS=true

# Normal logging level
VITE_LOG_LEVEL=info

# Rate limiting (more permissive than prod)
VITE_RATE_LIMIT_ENABLED=true
VITE_RATE_LIMIT_MAX=1000
